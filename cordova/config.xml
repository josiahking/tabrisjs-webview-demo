<?xml version='1.0' encoding='utf-8'?>
<widget id="josiahking.jobman" version="0.1.0">
  <name>Jobman</name>
  <description>
    Jobman app
  </description>
  <author email="josiahaccounts@gmail.com">
    Josiah King
  </author>
  <allow-navigation href="https://*.tempclousite.com/*" />
  <allow-navigation href="http://*.tempclousite.com/*" />
  <!-- Allow links to web pages to open in a browser -->
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <!-- Allow SMS links to open messaging app -->
  <allow-intent href="sms:*" />
  <!-- Allow tel: links to open the dialer -->
  <allow-intent href="tel:*" />
  <!-- Allow geo: links to open maps -->
  <allow-intent href="geo:*" />
  <!-- Allow mailto: links to open mail -->
  <allow-intent href="mailto:*" />
  <!-- Allow access to this URL for CSP -->
  <access origin="*" />
  <!--<access origin="https://*.google.com" />
  <access origin="https://*.hcaptcha.com" />
  <access origin="https://google.com" />
  <access origin="https://hcaptcha.com" />-->
  <preference name="EnableDeveloperConsole" value="$IS_DEBUG" />
  <plugin name="cordova-plugin-network-information" spec="^2.*" />
  <plugin name="cordova-plugin-whitelist" spec="1.3.*" />
  <plugin name="cordova-plugin-googleplus" spec="^5.*" >
    <variable name="REVERSED_CLIENT_ID" value="com.googleusercontent.apps.447391654587-q42jr9vsi1po5cinfqp3182nmou91udu" />
  </plugin>
</widget>
